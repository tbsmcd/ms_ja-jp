{
    "name": "MSLearn en-us -> ja-jp",
    "description": "MS Learn en-us -> ja-jp",
    "version": "0.1",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches": ["https://learn.microsoft.com/en-us/*"],
        "js": ["content.js"]
      }
    ],
    "permissions": ["storage"],
    "action": {
      "default_title": "MSLearn 日本語にリダイレクトする君",
      "default_popup": "popup.html"
    },
    "icons":
    {
      "16": "icon.png"
    }
}